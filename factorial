#include<iostream>
#include<fstream>
#include<math.h>
#include<cstdio>
#include<string>
using namespace std;
int factorial(int n);
int main()
    {
    int n;
    cin>>n;
    cout<<factorial(n)<<endl;
    return 0;
    }
int factorial(int n)
    {
    int i,f=1;
    if(n<=1)
        {
        return 1;
    }
    while(n>1)
        {
        return n*factorial(n-1);
    }
    return f;
}
